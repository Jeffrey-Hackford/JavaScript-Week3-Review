<h2>Questions:</h2>

<ul>
  {{#each model.questions as |question|}}
    {{#link-to 'question' question.id}}
      <div class="jumbotron">
        <h3>{{question.title}}</h3>
        <h5>Asked by: {{question.author}}</h5>
      </div>
    {{/link-to}}
  {{/each}}
</ul>
{{update-question title=title update="update"}}
{{new-question save2="save3"}}
